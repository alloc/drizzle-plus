{
  "include": ["src"],
  "exclude": [
    "src/generated/pg",
    "src/generated/mysql",
    "src/generated/sqlite"
  ],
  "compilerOptions": {
    "strict": true,
    "lib": ["esnext"],
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": []
  }
}
